<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission-sdk-23 android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_mylauncher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.VibrantsShopApplication"
        tools:targetApi="31">

        <activity
            android:name=".VibrantsPageApplication"
            android:exported="false"
            android:label="@string/title_activity_vibrants_page_application"
            android:theme="@style/Theme.VibrantsShopApplication.NoActionBar" />
        <activity
            android:name=".SignupApplication"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>